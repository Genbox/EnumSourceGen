//This tests if the transforms are being used.

[EnumSourceGen]
internal enum MyEnum
{
    [EnumTransformValue(NameOverride = "NotFirst")]
    First,

    [EnumTransformValue(TransformPreset = EnumTransform.UpperCase)]
    Second,

    [EnumTransformValue(Transform = "LUUUU")]
    Third,

    [EnumTransformValue(Transform = "/^../Something/")]
    Other
}